public class Test2{
    public static void main(String[] args){
        int n1=8;
	int n2=4;
	int sum1=n1+n2;
	int dif1=n1-n2;
	int prd1=n1*n2;
	int div1=n1/n2;
	int rem1=n1%n2;
	System.out.println(sum1);
  System.out.println(dif1);
  System.out.println(prd1);
  System.out.println(div1);
  System.out.println(rem1);
    }
}
